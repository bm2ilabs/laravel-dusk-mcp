{
    "name": "laravel-dusk-mcp",
    "version": "1.0.0",
    "description": "MCP server for Laravel Dusk browser testing",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
      "build": "tsc",
      "start": "node dist/index.js",
      "dev": "tsx src/index.ts",
      "prepare": "npm run build"
    },
    "keywords": [
      "mcp",
      "laravel",
      "dusk",
      "testing",
      "browser-testing"
    ],
    "author": "Your Name",
    "license": "MIT",
    "dependencies": {
      "@modelcontextprotocol/sdk": "^0.5.0",
      "glob": "^10.3.10"
    },
    "devDependencies": {
      "@types/glob": "^8.1.0",
      "@types/node": "^20.0.0",
      "tsx": "^4.7.0",
      "typescript": "^5.3.0"
    },
    "mcp": {
      "serverType": "stdio",
      "command": "node",
      "args": ["dist/index.js"],
      "env": {
        "LARAVEL_PATH": "/path/to/your/laravel/project"
      }
    }
  }