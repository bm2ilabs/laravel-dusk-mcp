{
    "name": "laravel-dusk-mcp",
    "version": "1.0.0",
    "description": "MCP server for Laravel Dusk browser testing",
    "main": "dist/index.js",
    "type": "module",
    "bin": {
      "laravel-dusk-mcp": "./dist/index.js"
    },
    "scripts": {
      "build": "tsc",
      "start": "node dist/index.js",
      "dev": "tsx src/index.ts",
      "prepare": "npm run build",
      "prepublishOnly": "npm run build"
    },
    "keywords": [
      "mcp",
      "laravel",
      "dusk",
      "testing",
      "browser-testing"
    ],
    "author": "BM2ilabs",
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "https://github.com/bm2ilabs/laravel-dusk-mcp.git"
    },
    "bugs": {
      "url": "https://github.com/bm2ilabs/laravel-dusk-mcp/issues"
    },
    "homepage": "https://github.com/bm2ilabs/laravel-dusk-mcp#readme",
    "files": [
      "dist"
    ],
    "dependencies": {
      "@modelcontextprotocol/sdk": "^0.5.0",
      "glob": "^10.3.10"
    },
    "devDependencies": {
      "@types/glob": "^8.1.0",
      "@types/node": "^20.0.0",
      "tsx": "^4.7.0",
      "typescript": "^5.3.0"
    },
    "mcp": {
      "serverType": "stdio"
    }
  }